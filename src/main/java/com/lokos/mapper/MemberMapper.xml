<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper

PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"

"http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.lokos.mapper.MemberMapper">

	<!-- 회원등록 -->
	<insert id="insertMemberInfoToDB">
		insert into members(memberEmail,memberPW,memberName,memberTel,memberRegdate,memberPolicyAgreement,memberTermsAgreement)
		values(#{memberEmail},#{memberPW},#{memberName},#{memberTel},#{memberRegdate},#{memberPolicyAgreement},#{memberTermsAgreement})
	</insert>
	
</mapper>